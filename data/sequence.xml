<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for lead mining requests -->
        <record id="ir_sequence_qr_code_product_template" model="ir.sequence">
            <field name="name">QR Code Product Template</field>
            <field name="code">qr.code.product.template</field>
            <field name="prefix">PROD_TMPL_</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="ir_sequence_qr_code_product_variant" model="ir.sequence">
            <field name="name">QR Code Product Variant</field>
            <field name="code">qr.code.product.variant</field>
            <field name="prefix">PRO_VARI_</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>